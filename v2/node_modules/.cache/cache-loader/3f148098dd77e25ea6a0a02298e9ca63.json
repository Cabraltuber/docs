dId\": \"applications/moodle-3\",\n  \"id\": \"applications/moodle-3\",\n  \"isDocsHomePage\": false,\n  \"title\": \"Moodle 3\",\n  \"description\": \"On this page, we explain step by step how to setup Moodle 3 with CloudPanel.\",\n  \"source\": \"@site/docs/applications/moodle-3.md\",\n  \"slug\": \"/applications/moodle-3\",\n  \"permalink\": \"/docs/cloudpanel-ce/applications/moodle-3\",\n  \"editUrl\": \"https://github.com/cloudpanel-io/docs/tree/master/cloudpanel-ce/docs/applications/moodle-3.md\",\n  \"version\": \"current\",\n  \"sidebar_label\": \"Moodle 3\",\n  \"sidebar\": \"sidebar\",\n  \"previous\": {\n    \"title\": \"Mautic 4\",\n    \"permalink\": \"/docs/cloudpanel-ce/applications/mautic-4\"\n  },\n  \"next\": {\n    \"title\": \"Neos 7\",\n    \"permalink\": \"/docs/cloudpanel-ce/applications/neos-7\"\n  }\n};\n  /* @jsx mdx */\nimport useBaseUrl from '@docusaurus/useBaseUrl';\nexport const rightToc = [{\n  value: 'Installation',\n  id: 'installation',\n  children: [{\n    value: 'Preparation',\n    id: 'preparation',\n    children: []\n  }, {\n    value: 'Installation',\n    id: 'installation-1',\n    children: []\n  }]\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n    <p>{`On this page, we explain step by step how to setup `}<strong parentName=\"p\"><a parentName=\"strong\" {...{\n          \"href\": \"https://moodle.org/\"\n        }}>{`Moodle 3`}</a></strong>{` with `}<strong parentName=\"p\">{`CloudPanel`}</strong>{`.`}</p>\n    <h2 {...{\n      \"id\": \"installation\"\n    }}>{`Installation`}</h2>\n    <p>{`In the following example we will setup `}<strong parentName=\"p\"><a parentName=\"strong\" {...{\n          \"href\": \"https://moodle.org/\"\n        }}>{`Moodle`}</a></strong>{` under the domain `}<strong parentName=\"p\"><em parentName=\"strong\">{`www.domain.com`}</em></strong>{`.`}</p>\n    <h3 {...{\n      \"id\": \"preparation\"\n    }}>{`Preparation`}</h3>\n    <p>{`Before we can start with the installation, we need to create an `}<a parentName=\"p\" {...{\n        \"href\": \"../frontend-area/users#adding-a-user\"\n      }}>{`SSH User`}</a>{`, a `}<a parentName=\"p\" {...{\n        \"href\": \"../frontend-area/databases#adding-a-database\"\n      }}>{`Database`}</a>{`, and a `}<a parentName=\"p\" {...{\n        \"href\": \"../frontend-area/domains#adding-a-domain\"\n      }}>{`Domain`}</a>{`.`}</p>\n    <p>{`When you `}<a parentName=\"p\" {...{\n        \"href\": \"../frontend-area/domains#adding-a-domain\"\n      }}>{`Add the Domain`}</a>{`, make sure to select the `}<strong parentName=\"p\">{`Moodle 3 Vhost Template`}</strong>{` and the right `}<strong parentName=\"p\">{`PHP Version`}</strong>{`.`}</p>\n    <img class=\"border\" src={useBaseUrl('img/v1/applications/moodle-3/new_domain.png')} /> <br /><br />\n    <h3 {...{\n      \"id\": \"installation-1\"\n    }}>{`Installation`}</h3>\n    <p>{`To install `}<strong parentName=\"p\">{`Moodle`}</strong>{` do the following steps:`}</p>\n    <ol>\n      <li parentName=\"ol\"><a parentName=\"li\" {...{\n          \"href\": \"../frontend-area/users#ssh-login\"\n        }}>{`Login via SSH`}</a>{` to the server e.g. with `}<strong parentName=\"li\">{`john-ssh`}</strong>{` and go to the users `}<strong parentName=\"li\">{`tmp`}</strong>{` directory:`}</li>\n    </ol>\n    <pre><code parentName=\"pre\" {...{}}>{`cd ~/tmp\n`}</code></pre>\n    <ol {...{\n      \"start\": 2\n    }}>\n      <li parentName=\"ol\">{`Download and extract the latest `}<strong parentName=\"li\">{`Moodle 3`}</strong>{` version.`}</li>\n    </ol>\n    <pre><code parentName=\"pre\" {...{}}>{`curl -sL https://download.moodle.org/download.php/direct/stable311/moodle-latest-311.tgz | tar xfz -\n`}</code></pre>\n    <ol {...{\n      \"start\": 3\n    }}>\n      <li parentName=\"ol\">{`Move files to the `}<strong parentName=\"li\">{`htdocs`}</strong>{` directory of the domain:`}</li>\n    </ol>\n    <pre><code parentName=\"pre\" {...{}}>{`cp -R moodle/* /home/cloudpanel/htdocs/www.domain.com/\n`}</code></pre>\n    <ol {...{\n      \"start\": 4\n    }}>\n      <li parentName=\"ol\">{`Reset permissions.`}</li>\n    </ol>\n    <pre><code parentName=\"pre\" {...{}}>{`cd /home/cloudpanel/htdocs/\nclpctl system:permissions:reset www.domain.com 775\n`}</code></pre>\n    <ol {...{\n      \"start\": 5\n    }}>\n      <li parentName=\"ol\">{`Clean up the `}<strong parentName=\"li\">{`tmp`}</strong>{` directory.`}</li>\n    </ol>\n    <pre><code parentName=\"pre\" {...{}}>{`rm -rf ~/tmp/*\n`}</code></pre>\n    <ol {...{\n      \"start\": 6\n    }}>\n      <li parentName=\"ol\">{`Open your domain in the browser and go through the `}<strong parentName=\"li\">{`installation wizard`}</strong>{`.`}</li>\n    </ol>\n    <img class=\"border\" src={useBaseUrl('img/v1/applications/moodle-3/installation_wizard.png')} />\n    <ol {...{\n      \"start\": 7\n    }}>\n      <li parentName=\"ol\">{`Done! `}<strong parentName=\"li\">{`Moodle 3`}</strong>{` is now installed.`}</li>\n    </ol>\n    <img class=\"border\" src={useBaseUrl('img/v1/applications/moodle-3/moodle_dashboard.png')} />\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}